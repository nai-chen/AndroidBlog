<?xml version="1.0" encoding="utf-8"?>
<resources>
   	<declare-styleable name="AttrTextView">
	    <attr name="attrReference" format="reference" />
	    <attr name="attrColor" format="color" />
	    <attr name="attrBoolean" format="boolean" />
	    <attr name="attrDimension" format="dimension" />
	    <attr name="attrFloat" format="float" />
	    <attr name="attrInteger" format="integer" />
	    <attr name="attrString" format="string" />
	    <attr name="attrFraction" format="fraction" />
	    <attr name="attrEnum" >
	        <enum name="east" value="1" />
	        <enum name="west" value="2" />
	        <enum name="south" value="3" />
	        <enum name="north" value="4" />
	    </attr>
	    <attr name="attrMulti" format="string|reference" />
	    <attr name="attrMultiDimen" format="dimension|reference" />
	</declare-styleable>
	
   	<declare-styleable name="GroupParamView">
   	    <attr name="text" format="string" />
   	    <attr name="textSize" format="integer"/>
   	</declare-styleable>

	<attr name="bg" format="color" />
	<declare-styleable name="Param1TextView">
		<attr name="bg"/>
	</declare-styleable>
	<declare-styleable name="Param2TextView">
		<attr name="bg"/>
	</declare-styleable>
   	
   	<declare-styleable name="Param3TextView">
   	    <attr name="android:orientation"/>
   	</declare-styleable>

	<declare-styleable name="CLeftSlideRemoveView">
		<attr name="layoutId" format="reference" />
	</declare-styleable>

	<declare-styleable name="ParamAssignView">
		<attr name="attr1" format="string" />
		<attr name="attr2" format="string" />
		<attr name="attr3" format="string" />
		<attr name="attr4" format="string" />
		<attr name="attr5" format="string" />
	</declare-styleable>

	<attr name="attrAssignStyle" format="reference" />

	<declare-styleable name="FlowLayout_Layout">
		<attr name="gravity" format="enum" >
			<enum name="top" value="0" />
			<enum name="center" value="1" />
			<enum name="bottom" value="2" />
		</attr>
	</declare-styleable>

</resources>
