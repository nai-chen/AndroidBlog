<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/view_content"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/btn_explicit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="explicit"/>

    <Button
        android:id="@+id/btn_implicit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="implicit"/>

    <Button
        android:id="@+id/btn_implicit_multi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="implicit multi"/>

    <Button
        android:id="@+id/btn_implicit_choose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="implicit choose"/>

    <Button
        android:id="@+id/btn_implicit_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="implicit category"/>

    <Button
        android:id="@+id/btn_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home"/>

    <Button
        android:id="@+id/btn_dial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dial"/>

    <Button
        android:id="@+id/btn_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="call"/>

    <Button
        android:id="@+id/btn_web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="web"/>

    <Button
        android:id="@+id/btn_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="photo"/>

    <Button
        android:id="@+id/btn_picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="picture"/>

</LinearLayout>